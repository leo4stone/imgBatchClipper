{
  "name": "imagemagickviewer",
  "version": "1.0.0",
  "description": "一个基于Electron和Vue.js的图片查看器应用",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --enable-logging",
    "build": "echo '构建功能待实现'",
